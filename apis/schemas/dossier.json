{
  "type": "object",
  "properties": {
    "dossier_id": {
      "type": "integer",
      "description": "Auto generated system reference"
    },
    "property_id": {
      "type": "integer",
      "description": "AML (system) property ID, External or Other reference"
    },
    "date_contract": {
      "type": "string",
      "description": "Format YYYY-MM-DD or YYYY/MM/DD"
    },
    "clients": {
      "type": "string",
      "description": "AML clients Ids, comma separated like 1,2,3"
    },
    "dossier_status": {
      "type": "string",
      "enum": ["open", "closed", "cancelled"]
    },
    "dossier_description": {
      "type": "string"
    },
    "purpose": {
      "type": "string",
      "enum": ["Primary", "Secondary", "Investment", "Other"]
    },
    "other_purpose": {
      "type": "string",
      "description": "required if purpose is Other"
    },
    "estimated_closing_date": {
      "type": "string",
      "description": "Format YYYY-MM-DD or YYYY/MM/DD"
    },
    "closing_date": {
      "type": "string",
      "description": "Actual closing date . Format YYYY-MM-DD or YYYY/MM/DD"
    },
    "sales_price": {
      "type": "integer",
      "description": null
    },
    "closing_costs": {
      "type": "integer",
      "description": null
    },
    "own_funds": {
      "type": "integer",
      "description": null
    },
    "financed_amount": {
      "type": "integer",
      "description": null
    },
    "total_cost": {
      "type": "integer",
      "description": null
    },
    "commission_included": {
      "type": "boolean",
      "description": null
    },
    "commission_value": {
      "type": "integer",
      "description": null
    },
    "commission_payer": {
      "type": "integer",
      "description": "AML client ID"
    },
    "assigned_to": {
      "type": "integer",
      "description": "AML user ID/email"
    }
  }
}
